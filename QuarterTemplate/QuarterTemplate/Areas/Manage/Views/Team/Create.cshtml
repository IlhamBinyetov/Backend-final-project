@model Team
@{
    ViewData["Title"] = "Create";
}


<div class="container-fluid">
    <h1>Create</h1>
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="Name">Name</label>
                <input asp-for="Name" class="form-control" placeholder="Name">
                <span class="text-danger" asp-validation-for="Name"></span>
            </div>
            <div class="form-group col-md-6">
                <label asp-for="Order">Order</label>
                <input asp-for="Order" class="form-control" placeholder="Order">
                <span class="text-danger" asp-validation-for="Order"></span>
            </div>
        </div>
        
        <div class="form-row">
            <div class="form-group col-md-4">
                <label asp-for="FacebookUrl">FacebookUrl</label>
                <input asp-for="FacebookUrl" class="form-control" placeholder="FacebookUrl">
                <span class="text-danger" asp-validation-for="FacebookUrl"></span>
            </div>
            <div class="form-group col-md-4">
                <label asp-for="LinkedinUrl">LinkedinUrl</label>
                <input asp-for="LinkedinUrl" class="form-control" placeholder="LinkedinUrl">
                <span class="text-danger" asp-validation-for="LinkedinUrl"></span>
            </div>
            <div class="form-group col-md-4">
                <label asp-for="TwitterUrl">TwitterUrl</label>
                <input asp-for="TwitterUrl" class="form-control" placeholder="TwitterUrl">
                <span class="text-danger" asp-validation-for="TwitterUrl"></span>
            </div>

        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label asp-for="ImageFile">ImageFile</label>
                <input asp-for="ImageFile" class="form-control" placeholder="ImageFile">
                <span class="text-danger" asp-validation-for="ImageFile"></span>
            </div>
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>

    </form>
</div>
